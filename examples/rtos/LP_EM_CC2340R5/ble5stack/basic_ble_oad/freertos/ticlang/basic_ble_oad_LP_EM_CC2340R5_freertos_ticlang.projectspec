            -l${COM_TI_SIMPLELINK_CC23XX_SDK_INSTALL_DIR}/source/ti/ble5stack_flash/lib_projects/CC2340R5/StackWrapper/lib/ticlang/m0p/StackWrapper.a
            -L${PROJECT_BUILD_DIR}/syscfg
            -lti/devices/cc23x0/driverlib/lib/ticlang/driverlib.a
            -Wl,--rom_model
            -Wl,--warn_sections
            -L${CG_TOOL_ROOT}/lib
            -llibc.a
        "
        postBuildStep="
            ${CG_TOOL_ROOT}/bin/tiarmobjcopy -O binary ${ProjName}.out ${ProjName}.bin --remove-section=.ccfg; ${COM_TI_SIMPLELINK_CC23XX_SDK_INSTALL_DIR}/tools/common/mcuboot/imgtool sign --header-size 0x100 --align 4 --slot-size 0x4BF00 --version 1.0.0 --pad-header --pad --key ${COM_TI_SIMPLELINK_CC23XX_SDK_INSTALL_DIR}/source/third_party/mcuboot/root-ec-p256.pem ${ProjName}.bin ${ProjName}.bin
        "
        enableSysConfigTool="true"
        sysConfigBuildOptions="
            --product ${COM_TI_SIMPLELINK_CC23XX_SDK_INSTALL_DIR}/.metadata/product.json
            --compiler ticlang
        "
        description="Basic BLE demo with oad application">

