<?xml version="1.0" encoding="UTF-8"?>
<projectSpec>
    <applicability>
        <when>
            <context
                deviceFamily="ARM"
                deviceId="Cortex M.CC2340R5"
            />
        </when>
    </applicability>

    <project
        title="basic_ble_oad app"
        name="basic_ble_oad_LP_EM_CC2340R5_freertos_ticlang"
        configurations="Release, Debug"
        toolChain="TICLANG"
        connection="TIXDS110_Connection.xml"
        device="Cortex M.CC2340R5"
        ignoreDefaultDeviceSettings="true"
        ignoreDefaultCCSSettings="true"
        products="com.ti.SIMPLELINK_CC23XX_SDK;sysconfig"
        compilerBuildOptions="
            -I${PROJECT_ROOT}
            -I${PROJECT_ROOT}/${ConfigName}
            -I${PROJECT_LOC}/app
            -I${COM_TI_SIMPLELINK_CC23XX_SDK_INSTALL_DIR}/source/third_party/mcuboot/boot/bootutil/include
            -I${COM_TI_SIMPLELINK_CC23XX_SDK_INSTALL_DIR}/source/third_party/mcuboot/boot/ti/source/mcuboot_app
            -I${COM_TI_SIMPLELINK_CC23XX_SDK_INSTALL_DIR}/source/third_party/mcuboot/boot/ti/source/mcuboot_app/onchip
            -I${COM_TI_SIMPLELINK_CC23XX_SDK_INSTALL_DIR}/source/ti/ble5stack_flash/profiles/oad/cc26xx
            -I${COM_TI_SIMPLELINK_CC23XX_SDK_INSTALL_DIR}/source/ti/common/cc26xx/flash_interface/
            -I${COM_TI_SIMPLELINK_CC23XX_SDK_INSTALL_DIR}/source/ti/drivers/nvs
            -I${COM_TI_SIMPLELINK_CC23XX_SDK_INSTALL_DIR}/source/ti/devices/cc23x0/driverlib
            -DMCUBOOT_ENABLE
            -DEXCLUDE_OAD_OFFCHIP
            -DOAD_CFG
            -DICALL_NO_APP_EVENTS
            -mcpu=cortex-m4
            -mthumb
            -mlittle-endian
            -std=gnu99
            -std=c99
            -ffunction-sections
            -g
            -Oz
            -I${COM_TI_SIMPLELINK_CC23XX_SDK_INSTALL_DIR}/source
            -I${COM_TI_SIMPLELINK_CC23XX_SDK_INSTALL_DIR}/source/ti
            -I${COM_TI_SIMPLELINK_CC23XX_SDK_INSTALL_DIR}/source/ti/ble5stack_flash
            -I${COM_TI_SIMPLELINK_CC23XX_SDK_INSTALL_DIR}/source/ti/ble5stack_flash/controller/cc26xx/inc
            -I${COM_TI_SIMPLELINK_CC23XX_SDK_INSTALL_DIR}/source/ti/ble5stack_flash/inc
            -I${COM_TI_SIMPLELINK_CC23XX_SDK_INSTALL_DIR}/source/ti/ble5stack_flash/rom
            -I${COM_TI_SIMPLELINK_CC23XX_SDK_INSTALL_DIR}/source/ti/ble5stack_flash/common/cc26xx
