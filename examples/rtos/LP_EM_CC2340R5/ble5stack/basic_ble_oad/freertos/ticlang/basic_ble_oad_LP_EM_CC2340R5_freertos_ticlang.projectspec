            -I${COM_TI_SIMPLELINK_CC23XX_SDK_INSTALL_DIR}/source/ti/ble5stack_flash/icall/src/inc
            -I${COM_TI_SIMPLELINK_CC23XX_SDK_INSTALL_DIR}/source/ti/ble5stack_flash/npi/src
            -I${COM_TI_SIMPLELINK_CC23XX_SDK_INSTALL_DIR}/source/ti/ble5stack_flash/osal/src/inc
            -I${COM_TI_SIMPLELINK_CC23XX_SDK_INSTALL_DIR}/source/ti/ble5stack_flash/services/src/saddr
            -I${COM_TI_SIMPLELINK_CC23XX_SDK_INSTALL_DIR}/source/ti/ble5stack_flash/services/src/sdata
