             -I${FREERTOS_INSTALL_DIR}/FreeRTOS/Source/portable/GCC/ARM_CM0
             -I${COM_TI_SIMPLELINK_CC23XX_SDK_INSTALL_DIR}/kernel/freertos
             -DFREERTOS
             -DNVOCMP_POSIX_MUTEX
            -gdwarf-3
            -mcpu=cortex-m0plus
            -march=thumbv6m
            -mfloat-abi=soft
        "
        linkerBuildOptions="
            --diag_wrap=off
            --define=OAD_CFG=1
            --display_error_number
            -x
            --diag_suppress=16002-D
            --diag_suppress=10247-D
            --diag_suppress=10325-D
            --diag_suppress=10229-D
            --diag_suppress=16032-D
            -c
            -L${COM_TI_SIMPLELINK_CC23XX_SDK_INSTALL_DIR}/source
            -l${COM_TI_SIMPLELINK_CC23XX_SDK_INSTALL_DIR}/source/ti/devices/cc23x0/driverlib/lib/ticlang/driverlib.a
            -l${COM_TI_SIMPLELINK_CC23XX_SDK_INSTALL_DIR}/source/third_party/spiffs/lib/ticlang/m0p/spiffs.a
