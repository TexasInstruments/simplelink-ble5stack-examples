            @${COM_TI_SIMPLELINK_CC23XX_SDK_INSTALL_DIR}/source/ti/ble5stack_flash/config/factory_config.opt
            -DCC23X0
            -DDeviceFamily_CC23X0
            -DNVOCMP_NWSAMEITEM=1
            -DFLASH_ONLY_BUILD
            -DUSE_RCL
            -I${COM_TI_SIMPLELINK_CC23XX_SDK_INSTALL_DIR}/source/ti/devices/cc23x0
